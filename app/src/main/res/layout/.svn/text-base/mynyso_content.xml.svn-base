    android:id="@+id/ll_mynyso_content"
    android:paddingLeft="@dimen/view_toview_two"
    android:paddingRight="@dimen/view_toview_two"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
