                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="36dp">
                        <ImageView
                            android:src="@mipmap/order_dfk"
                            android:scaleType="fitXY"
                            android:layout_centerInParent="true"
                            android:layout_width="22dp"
                            android:layout_height="22dp" />

                        <View
                            android:id="@+id/iv_order_dfk"
                            android:layout_width="6dp"
                            android:layout_height="6dp"
                            android:layout_centerInParent="true"
                            />
                        <TextView
                            android:visibility="gone"
                            android:id="@+id/tv_order_dfk_dot"
                            android:singleLine="true"
                            android:layout_toRightOf="@+id/iv_order_dfk"
                            android:layout_above="@+id/iv_order_dfk"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_textview_order_state"
                            android:gravity="center"
                            android:textSize="8sp"
                            android:textColor="@color/colorWhite" />
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="待付款"
                        android:textColor="@color/colorBlack"
                        android:textSize="12sp" />
                </LinearLayout>
                <LinearLayout
